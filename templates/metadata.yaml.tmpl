local-hostname: ${hostname}
instance-id: ${hostname}
network:
  version: 2
  ethernets:
    ens192:
      %{ if ip_address == false }dhcp4: true
      %{ else }dhcp4: false
      addresses:
        - ${ip_address}/${netmask}
      gateway4: ${gateway}
      nameservers:
        addresses: [ for nameserver in nameservers: ${nameserver} ]
      %{ endif }

      
